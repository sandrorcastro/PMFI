@using ITBI.Entities.DBProsiga;
@implements IDialogContentComponent<Logradouro>


    
<FluentCard Justify="@JustifyContent.Center" >
    <FluentTextField @bind-Value="@Content.LogradTipo">Tipo de Logradouro:</FluentTextField>
    <FluentTextField @bind-Value="@Content.LogradNome">Logradouro:</FluentTextField>
    <FluentTextField @bind-Value="@Numero">Número:</FluentTextField>
    <FluentTextField @bind-Value="@Bairro">Bairro:</FluentTextField>


</FluentCard>
@code {
    [Parameter]
    public Logradouro Content { get; set; } = default!;
    [Parameter]
    public string? Numero { get; set; }
    [Parameter]
    public string? Bairro { get; set; }
    [CascadingParameter]
    public FluentDialog? Dialog { get; set; }

 
}

