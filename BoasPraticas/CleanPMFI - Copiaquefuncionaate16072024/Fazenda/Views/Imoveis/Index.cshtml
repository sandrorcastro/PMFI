            <th>
                @Html.DisplayNameFor(model => model.Ocupa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Posicao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Situacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Terreno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoCon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Utiliz)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InscricaoImobiliaria)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.EdifCaracteristica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CategId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdifAreaConstruida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdifAreaCoberta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdifFracaoIdealEdificacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdifPontosEdificacao)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.ValorVenalCalculo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorVenalEdificacao)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.ValorVenalTerrenoParaEdificacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inci.InciNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ocupa.OcupaNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Posicao.PosicaoNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Situacao.SituacaoNome)
            </td>
