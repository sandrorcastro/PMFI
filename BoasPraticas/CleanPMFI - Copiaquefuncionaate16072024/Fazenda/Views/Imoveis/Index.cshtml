@model IEnumerable<Fazenda.Entities.DBProsiga.TribEdificaco>

@{
    ViewData["Title"] = "Index";
}

<h1>Relatório</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InscricaoImobiliaria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdifCaracteristica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CategId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdifAreaConstruida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdifAreaCoberta)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.EdifFracaoIdealEdificacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdifPontosEdificacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorVenalCalculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorVenalEdificacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorVenalTerrenoParaEdificacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Inci)
