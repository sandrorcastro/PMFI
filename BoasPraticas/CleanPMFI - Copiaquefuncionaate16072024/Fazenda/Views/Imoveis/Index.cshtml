                @Html.DisplayFor(modelItem => item.Terreno.TerrenoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoCon.TipoConNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uso.UsoNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Utiliz.UtilizNome)
            </td>
            <td>
           
                <a asp-action="Details" asp-route-id="@item.EdificacaoId">Detalhes</a>
                <a asp-action="SimularIPTU" asp-route-id="@item.EdificacaoId">SimularIPTUs</a>

            </td>
        </tr>
