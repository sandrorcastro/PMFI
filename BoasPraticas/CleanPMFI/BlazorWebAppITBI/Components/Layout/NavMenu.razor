@rendermode InteractiveServer

<div class="navmenu">
    <input type="checkbox" id="navmenu-toggle" />
    <nav class="sitenav" aria-labelledby="main-menu" onclick="document.getElementById('navmenu-toggle').click();">
        <FluentNavMenu Id="main-menu" Collapsible="true" Width="250" Title="Navigation menu" @bind-Expanded="expanded">
            <FluentNavLink Href="/" Match="NavLinkMatch.All" Icon="@(new Icons.Regular.Size20.DocumentAdd())" IconColor="Color.Accent">AutoDeclaração</FluentNavLink>
        </FluentNavMenu>
    </nav>
</div>

             <FluentOverlay @bind-Visible=@_overlayIsVisible
                       Opacity="0.4"
                       Alignment="Align.Center"
                       Justification="@JustifyContent.Center">
            <FluentProgressRing />
        </FluentOverlay>

@code {
    private bool expanded = true;
    private bool _overlayIsVisible = false;
