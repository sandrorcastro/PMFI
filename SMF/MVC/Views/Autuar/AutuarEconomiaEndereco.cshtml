@model IEnumerable<Domain.Entities.Endereco_Entidade>
@{
    ViewData["Title"] = "Details";
}



<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CEP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endereco.Logradouro.TipoLogradouro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endereco.Logradouro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endereco.Logradouro.Bairro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endereco.Logradouro.Cidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correspondencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Principal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ativo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CEP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Endereco.Logradouro.TipoLogradouro.Descricao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Endereco.Logradouro.Descricao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Numero)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Endereco.Logradouro.Bairro.Descricao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Endereco.Logradouro.Cidade.Descricao)
                </td>
               
                <td>
                    @Html.DisplayFor(modelItem => item.Correspondencia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Principal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ativo)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                    @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                    @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                </td>
            </tr>
        }
    </tbody>
</table>
